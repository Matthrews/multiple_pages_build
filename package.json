{
  "name": "multiple_pages_build",
  "version": "1.0.0",
  "description": "多页面打包实践",
  "main": "index.js",
  "scripts": {
    "dev": "node scripts/build/dev",
    "start": "cross-env NODE_ENV=development BUILD_ENV=dev npm run dev",
    "build": "node scripts/build/prod",
    "build:fat": "cross-env NODE_ENV=production BUILD_ENV=fat npm run build",
    "build:uat": "cross-env NODE_ENV=production BUILD_ENV=uat npm run build",
    "build:pro": "cross-env NODE_ENV=production BUILD_ENV=pro npm run build",
    "fat": "npm run build:fat",
    "uat": "npm run build:uat",
    "pro": "npm run build:pro",
    "lint:js": "eslint --fix --ext .tsx,.jsx,ts,js src",
    "lint:css": "stylelint './src/**/*.{css,less}'",
    "add:page": "node scripts/addPage",
    "remove:page": "node scripts/removePage",
    "lint:staged": "lint-staged",
    "analyz": "cross-env npm_config_report=true npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cb": "node scripts/git"
  },
  "dependencies": {
    "alloyfinger": "0.1.16",
    "axios": "0.20.0",
    "classnames": "2.2.6",
    "file-saver": "2.0.5",
